{
    "name": "meniam/model",
    "type": "library",
    "description": "Database models for MySql",
    "keywords": ["model","mysql", "orm"],
    "homepage": "http://github.com/meniam/model",
    "license": "MIT",
    "authors": [
        {
        "name": "Eugene Myazin",
        "email": "meniam@gmail.com",
        "homepage": "http://meniam.ru",
        "role": "Maintainer"
        }
    ],
    "require":{
        "php"           :">=5.4.0,<5.6",

        "doctrine/annotations"   : ">=1.0",

        "zendframework/zend-validator": "2.3.*",
        "zendframework/zend-console": "2.3.*",
        "zendframework/zend-code": "2.3.*",
        "zendframework/zend-uri": "2.3.*",

        "phpunit/phpunit": "3.7.22",
        "phpunit/php-invoker" : "1.1.*",
        "phpunit/php-token-stream": "1.2.*@dev",
        "phpunit/php-timer": "1.0.5"

    },

    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev",
            "dev-develop": "2.3-dev"
        }
    },

    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "bin": ["bin/models"],
    "autoload": {
        "classmap": [
            "library/"
        ]
    }
}